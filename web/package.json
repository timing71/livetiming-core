{
  "name": "livetiming",
  "version": "0.2.1",
  "description": "Live timing aggregator front-end",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build/*",
    "build": "webpack",
    "start": "webpack-dev-server",
    "upload": "rsync --delete -rz build/* timing.71wytham.org.uk:/var/www/livetiming",
    "deploy": "npm run clean && npm run build && npm run upload"
  },
  "author": "James Muscat",
  "license": "ISC",
  "dependencies": {
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.3.0",
    "moment": "^2.13.0",
    "react": "^15.0.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.0.2",
    "react-ga": "^1.4.1",
    "react-router": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.12.0",
    "html-webpack-template": "^5.0.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "start": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  }
}
